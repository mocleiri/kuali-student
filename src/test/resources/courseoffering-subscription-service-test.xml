<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd
          http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.2.xsd">

    <bean id="courseOfferingSubscriptionServiceDecorator" class="org.kuali.student.enrollment.courseoffering.service.CourseOfferingSubscriptionServiceDecorator">
        <property name="nextDecorator" ref="courseOfferingServiceMapImpl"/>
    </bean>

    <bean id="courseOfferingCallbackService" class="org.kuali.student.enrollment.courseoffering.service.CourseOfferingCallbackServiceImpl">
        <property name="courseSeatCountService" ref="courseSeatCountService"/>
        <property name="courseOfferingService" ref="courseOfferingServiceMapImpl"/>
    </bean>

    <bean id="courseSeatCountService" class="org.kuali.student.enrollment.courseseatcount.service.CourseSeatCountServiceImpl">
    </bean>

    <bean id="courseOfferingServiceMapImpl" class="org.kuali.student.enrollment.courseoffering.service.CourseOfferingServiceMapImpl">
    </bean>
</beans>