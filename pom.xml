    <groupId>org.kuali.maven.plugins</groupId>
    <artifactId>maven-kscontractdoc-plugin</artifactId>
    <packaging>maven-plugin</packaging>
    <version>0.0.1-SNAPSHOT</version>
    <name>KS Contract Documentation Generator</name>
    <description>Generates documentation for Kuali Student service contracts</description>
    <properties>
<!--        <cxf.version>2.2.10</cxf.version>-->
        <kuali.rice.version>2.0.0-b1</kuali.rice.version>
    </properties>
    <repositories>
        <repository>
            <id>kuali.nexus</id>
            <name>Nexus Repository Manager</name>
            <url>http://nexus.kuali.org/content/groups/public</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
