<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<import resource="classpath:UifKSDefinitions.xml"/>	
	
	<bean id="KS-OrgInfoManage-View" parent="KS-Uif-FormView">
		<property name="id" value="orgInfoManageView" />
		<property name="headerText" value="Organization Information" />
		<property name="formClass"	value="org.kuali.student.core.organization.ui.form.OrgInfoAdminSearchForm" />
		<property name="viewHelperServiceClass" value="org.kuali.student.core.organization.ui.utils.OrganizationServiceHelper" />
		<property name="renderHeader" value="false" />
		<property name="renderFooter" value="false" />		
		<property name="formPostUrl" value="orgInfoAdminManage" />
		<property name="items">
			<list>
				<ref bean="KS-OrgInfoSearch-View" />
				<ref bean="KS-OrgInfoDetail-View" />
				<ref bean="KS-OrgInfoEdit-View" />
				<ref bean="KS-OrgInfoCreate-View" />
			</list>
		</property>
	</bean>
</beans>