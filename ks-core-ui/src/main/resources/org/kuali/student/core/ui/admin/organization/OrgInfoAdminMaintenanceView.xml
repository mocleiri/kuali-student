<beans xmlns="http://www.springframework.org/schema/beans" xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">  
  <bean id="OrgInfoAdmin-MaintenanceView" parent="Uif-MaintenanceView">
  	<property name="headerText" value="OrgInfoAdmin Maintenance"/>
   	<property name="dataObjectClassName" value="org.kuali.student.r2.core.organization.dto.OrgInfo"/>
    <property name="items">
      <list>
        <bean parent="Uif-MaintenanceGridSection">
          <property name="headerText" value="Create a New OrgInfoAdmin"/>
          <property name="items">
            <list>
              <bean parent="Uif-InputField" p:propertyName="id" p:readOnly="true"/>
              <bean parent="Uif-InputField" p:propertyName="typeKey" >
              	<property name="quickfinder.dataObjectClassName" value="org.kuali.student.r2.core.class1.type.dto.TypeInfo"/>
                <property name="quickfinder.fieldConversions">
                   <map>
                       <entry key="key" value="typeKey"/>
                   </map>
                </property>                  
              </bean>   
              <bean parent="Uif-InputField" p:propertyName="stateKey" >
              	<property name="quickfinder.dataObjectClassName" value="org.kuali.student.r2.core.class1.state.dto.StateInfo"/>
                <property name="quickfinder.fieldConversions">
                   <map>
                       <entry key="key" value="stateKey"/>
                   </map>
                </property>                  
              </bean>  
              <bean parent="Uif-InputField" p:propertyName="longName" />
              <bean parent="Uif-InputField" p:propertyName="longDescr" />
              <bean parent="Uif-InputField" p:propertyName="shortName" />
              <bean parent="Uif-InputField" p:propertyName="shortDescr" />              
              <bean parent="Uif-InputField" p:propertyName="effectiveDate" />
              <bean parent="Uif-InputField" p:propertyName="expirationDate" />              		                  	
            </list>
          </property>
        </bean>        
      </list>
    </property>
  </bean>
  <bean id="OrgInfoAdminMaintenanceDocument" parent="OrgInfoAdminMaintenanceDocument-parentBean"/>
  <bean id="OrgInfoAdminMaintenanceDocument-parentBean" abstract="true" parent="uifMaintenanceDocumentEntry">
    <property name="dataObjectClass" value="org.kuali.student.r2.core.organization.dto.OrgInfo"/>
    <property name="maintainableClass" value="org.kuali.student.core.ui.admin.organization.OrgInfoAdminMaintainableImpl"/>
    <property name="businessRulesClass" value="org.kuali.student.core.ui.admin.organization.rule.OrgInfoAdminRule"/>
    <property name="documentTypeName" value="OrgInfoAdminMaintenanceDocument"/>
     <property name="lockingKeys">
        <list>
            <value>id</value>
        </list>
    </property>
  </bean>
</beans>