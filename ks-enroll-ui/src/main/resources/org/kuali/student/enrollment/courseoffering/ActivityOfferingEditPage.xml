    <bean id="ActivityOfferingEdit-submitSection" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <bean parent="Uif-InputField-LabelRight" p:cssClasses="unprocessed_changes_checkbox" p:render="@{#dp.schedulingCompleted}" p:label="Send revised delivery logistics to the scheduler for processing upon save." p:propertyName="sendRDLsToSchedulerAfterMSE">
                    <property name="control">
                        <bean parent="Uif-CheckboxControl"/>
                    </property>
                </bean>
                <ref bean="ActivityOfferingEdit_SubmitCancel"/>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit_SubmitCancel" parent="ActivityOfferingEdit_SubmitCancel-parent"/>
    <bean id="ActivityOfferingEdit_SubmitCancel-parent" parent="Uif-DocumentPageFooter" abstract="true">
        <property name="items">
            <list>
                <bean parent="Uif-PrimaryActionButton" p:ajaxSubmit="false" p:methodToCall="route" p:actionLabel="Update"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_ROUTE]}" p:preSubmitCall="return preSubmitCheck();"/>
                <bean parent="Uif-SecondaryActionButton" p:ajaxSubmit="false" p:methodToCall="route" p:actionLabel="Save Progress"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_ROUTE]}" p:preSubmitCall="return preSubmitCheck();">
                    <property name="actionParameters">
                      <map>
                        <entry key="aoId" value="@{#dp.aoInfo.id}"/>
                      </map>
                    </property>
                </bean>
                <bean parent="Uif-ActionLink" p:ajaxSubmit="false" p:methodToCall="cancel" p:actionLabel="Cancel"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CANCEL]}"/>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-section1" parent="Uif-VerticalBoxSection" >
        <property name="items">
            <list>
                <bean  parent="Uif-GridGroup"  p:layoutManager.numberOfColumns="4" p:additionalCssClasses="ks-gray-background,ks-no-margins">
                    <property name="items">
                        <list>
                            <bean parent="Uif-DataField" p:colSpan="3" id="status" p:label="Status" p:fieldLabel.renderColon ="false" p:propertyName="stateName"/>
                            <bean parent="Uif-DataField" p:colSpan="3" id="term" p:label="Term" p:fieldLabel.renderColon="false" p:propertyName="termName" p:readOnly="true"/>
                            <bean parent="Uif-DataField" id="subterm_name" p:label="Sub Term" p:fieldLabel.renderColon="false" p:propertyName="subTermName"  p:readOnly="true"/>
                            <bean parent="Uif-HorizontalBoxSection" p:colSpan="2">
                                <property name="items">
                                    <list>
                                        <bean parent="Uif-NavigationActionLink" id="change_link" p:actionLabel="Change" p:render="@{#dp.hasSubTerms and !#parent.readOnly}"
                                              p:actionScript="openPopupContent(e,'KS-ActivityOfferingEdit-SubtermsPopupForm');" p:componentSecurity.performActionAuthz="true" p:actionEvent="changeSubtermAO"/>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-DataField" p:colSpan="3" id="start_end_date" p:label="Start/End"  p:readOnly="true"
                                  p:fieldLabel.renderColon="false" p:propertyName="termStartEndDate" />

                            <bean parent="Uif-DataField" id="type_name" p:label="Activity Type" p:fieldLabel.renderColon="false" p:propertyName="typeName" p:readOnly="true"/>
                            <bean parent="Uif-DataField"  id="format_offering_name" p:label="Format" p:fieldLabel.renderColon="false" p:propertyName="aoInfo.formatOfferingName" p:readOnly="true"/>
                        </list>
                    </property>
                </bean>
                <bean id="KS-ActivityOfferingEdit-SubtermsPopupForm" parent="Uif-HorizontalBoxSection">
                    <property name="hidden" value="true"/>
                    <property name="cssClasses">
                        <list>
                            <value>uif-bubblepopup-content</value>
                        </list>
                    </property>
                    <property name="items">
                        <list>
                            <bean id="subterm_type" parent="Uif-InputField-LabelTop" p:propertyName="subTermId" p:label="Choose a subterm">
                                <property name="control">
                                    <bean parent="Uif-DropdownControl" p:onChangeScript="displayAOsubTerm('subterm_name','subterm_type','change_link','activityCode','@{#dp.subTermDatesJsonString}');"/>
                                </property>
                                <property name="optionsFinder">
                                    <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.ActivityOfferingSubtermKeyValues" p:addBlankOption="false"/>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <ref bean="ActivityOfferingEdit-submitSection"/>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-section2" parent="Uif-VerticalBoxSection" p:onDocumentReadyScript="calculatePercent()">
        <property name="headerText" value="Activity Offering Information"/>
        <property name="items">
            <list>
                <bean parent="Uif-InputField" id="activity_code" p:label="Activity Code " p:fieldLabel.renderColon="false" p:propertyName="aoInfo.activityCode"
                      p:id="activityCode" p:componentSecurity.editAuthz="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="1" p:disabled="@{!#dp.editActivityCodeAllowed}" />
                    </property>
                </bean>
                <bean parent="Uif-InputField" p:propertyName="partOfColoSetOnLoadAlready" p:id="colocatedAO_value">
                    <property name="control">
                        <bean parent="Uif-HiddenControl"/>
                    </property>
                </bean>
                <bean parent="Uif-InputField" p:propertyName="hiddenMaxEnrollmentShared" p:id="maxEnrollmentShared_value">
                    <property name="control">
                        <bean parent="Uif-HiddenControl"/>
                    </property>
                </bean>
                <bean parent="Uif-InputField-LabelRight" p:label="This activity is co-located" p:fieldLabel.renderColon="false" p:propertyName="colocatedAO"
                      p:id="is_co_located" p:componentSecurity.viewAuthz="true">
                    <property name="control">
                        <bean parent="Uif-CheckboxControl">
                            <!--<property name="onChangeScript" value="setupColoCheckBoxChange(this,true)"/>-->
                            <property name="onChangeScript" value="handleColocation();"/>
                        </bean>
                    </property>
                </bean>
                <bean parent="Uif-InputField" id="max_enrollment" p:label="Total Maximum Enrollment" p:fieldLabel.renderColon="false" p:propertyName="aoInfo.maximumEnrollment"
                      p:id="maximumEnrollment" p:onChangeScript="calculatePercent(jQuery(this))" p:componentSecurity.editAuthz="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="3"/>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-CoLocation" parent="Uif-VerticalBoxSection"  p:componentSecurity.viewAuthz="true"
          p:style="width:600px;border: 1px solid black;" p:layoutManager.style="margin-left:10px;margin-right:10px;">
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="Select Activities for Colocation" p:style="font-size:15px;"/>
                <ref bean="ActivityOfferingEdit-CoLocatedActivities"/>
                <bean parent="Uif-Message" p:messageText="Set Maximum Enrollment" p:style="font-size:15px;"/>
                <bean id="share_seats" parent="Uif-InputField" p:propertyName="maxEnrollmentShared">
                    <property name="control">
                        <bean parent="Uif-HorizontalRadioControl">
                            <property name="options">
                                <list>
                                    <bean parent="Uif-KeyLabelPair" p:key="true" p:value="Share max enrollment across all activities"/>
                                    <bean parent="Uif-KeyLabelPair" p:key="false" p:value="Manage Enrollments separately"/>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
                <ref bean="ActivityOfferingEdit-CoLocatedEnrollmentShared"/>
                <ref bean="ActivityOfferingEdit-CoLocatedEnrollmentSeperate"/>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-CoLocatedActivities" parent="Uif-TableCollectionSection" p:layoutManager.style="width:150px;">
        <property name="header.render" value="false"/>
        <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.ColocatedActivity" />
        <property name="propertyName" value="colocatedActivities"/>
        <property name="layoutManager.richTable.render" value="false"/>
        <property name="layoutManager.renderSequenceField" value="false" />
        <property name="addLineActions">
            <list>
                <!--<bean parent="Uif-AddLineAction" p:successCallback="addColocatedAOSuccessCallBack();" p:actionScript="addColocatedAO(this);"/>-->
                <bean parent="Uif-AddLineAction" p:successCallback="addColocatedAOSuccessCallBack();"/>
            </list>
        </property>
        <property name="lineActions">
            <list>
                <bean parent="Uif-DeleteLineAction" p:successCallback="addColocatedAOSuccessCallBack();" p:render="@{#dp.requestedScheduleComponents.isEmpty()}"/>
            </list>
        </property>
        <property name="items">
            <list>

                <!-- CO Input Field -->
                <bean parent="Uif-InputField"
                      p:id="ActivityOfferingEdit-CoLocatedActivities-CoInputField"
                      p:propertyName="courseOfferingCode"
                      p:fieldLabel.render="false"
                      p:uppercaseValue="true"
                      p:required="true"
                      p:readOnly="@{!#isAddLine}">

                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="10" p:watermarkText="ENGL101"/>
                    </property>
                    <property name="suggest">
                        <bean parent="Uif-Suggest" p:render="true" p:returnFullQueryObject="true">
                            <property name="suggestQuery">
                                <bean parent="Uif-AttributeQueryConfig" p:queryMethodToCall="retrieveCourseOfferingCode" p:queryMethodArgumentFieldList="document.newMaintainableObject.dataObject.termId"/>
                            </property>
                            <property name="templateOptions">
                                <map merge="true">
                                    <entry key="minLength" value="4"/>
                                </map>
                            </property>
                        </bean>
                    </property>
                </bean>

                <!-- AO Input Field -->
                <bean parent="Uif-InputField"
                      p:id="ActivityOfferingEdit-CoLocatedActivities-AoInputField"
                      p:propertyName="activityOfferingCode"
                      p:fieldLabel.render="false"
                      p:uppercaseValue="true"
                      p:required="true"
                      p:readOnly="@{!#isAddLine}">

                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="4" p:watermarkText="A"/>
                    </property>
                    <property name="quickfinder.dataObjectClassName" value="org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingWrapper"/>
                    <property name="quickfinder.quickfinderAction">
                        <bean parent="Uif-ActionLink" p:actionLabel="Advanced Select" p:style="display:block" p:methodToCall="performLookup"/>
                    </property>
                    <property name="quickfinder.autoSearch" value="true"/>
                    <property name="quickfinder.fieldConversions">
                        <map>
                            <entry key="aoInfo.activityCode" value="activityOfferingCode"/>
                            <entry key="aoInfo.id" value="aoId"/>
                        </map>
                    </property>
                    <property name="quickfinder.lookupParameters">
                        <map>
                            <entry key="courseOfferingCode" value="courseOfferingCode"/>
                            <entry key="editRenderHelper.termId" value="termId"/>
                        </map>
                    </property>
                </bean>

            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-CoLocatedEnrollmentShared" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <bean id="shared_max_enr" parent="Uif-InputField" p:propertyName="sharedMaxEnrollment" p:label="Shared Maximum Enrollment" p:required="true" p:exclusiveMin="0">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="4" />
                    </property>
                </bean>
                <bean parent="Uif-Message"
                      p:messageText="[image src=&quot;@{#ConfigProperties['krad.externalizable.images.url']}/validation/info.png&quot;]  Max enrollment is shared by all co-located activities. [br] Any changes made here will update the other activities on save."
                      p:style="background-color:#C8E4F7; padding-top:4px; padding-bottom:4px;font-size: 110%;border: 1px solid rgb(9, 172, 248);" p:colSpan="2"/>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-CoLocatedEnrollmentSeperate" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <bean id="enr_shared_table" parent="Uif-TableCollectionSection" p:style="width:250px;float:right;" p:forceSessionPersistence="true" >
                    <property name="headerText" value="Combined Maximum Enrollment"/>
                    <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.ColocatedActivity" />
                    <property name="propertyName" value="editRenderHelper.manageSeperateEnrollmentList"/>
                    <property name="renderAddLine" value="false"/>
                    <property name="header">
                        <bean parent="Uif-HeaderFour"/>
                    </property>
                    <property name="renderLineActions" value="false"/>
                    <property name="layoutManager.renderSequenceField" value="false"/>
                    <property name="layoutManager.richTable.render" value="false" />
                    <!--<property name="layoutManager.columnCalculations">
                          <list>
                            <bean parent="Uif-ColumnCalculationInfo-Sum" p:showGroupTotal="false" p:propertyName="maxEnrollmentCount" p:calculateOnKeyUp="true"/>
                          </list>
                    </property>-->
                    <property name="items">
                        <list>
                            <bean parent="Uif-DataField" p:propertyName="editRenderHelper.code" p:label="Colocated Activities" p:style="color:green;"/>
                            <bean parent="Uif-InputField" p:propertyName="maxEnrollmentCount" p:label="Max Erollment" p:readOnly="@{!#lp.editRenderHelper.allowEnrollmentEdit}" p:required="true" p:exclusiveMin="0">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="4" />
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <bean parent="Uif-Message"
                      p:messageText="[image src=&quot;@{#ConfigProperties['krad.externalizable.images.url']}/validation/info.png&quot;]  The max enrollments for all co-located activities will be combined. Any changes may effect room availability."
                      p:style="background-color:#C8E4F7; padding-top:4px; padding-bottom:4px;font-size: 110%;border: 1px solid rgb(9, 172, 248);float:center;" p:colSpan="2"/>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-PersonnelSection" parent="Uif-VerticalBoxSection">
        <property name="headerText" value="Personnel"/>
        <property name="items">
            <list>
                <bean id="ActivityOffering-PersonnelSection" parent="Uif-TableCollectionSection"
                      p:layoutManager.numberOfColumns="6"
                      p:id="ao-personnelgroup"
                      p:layoutManager.style="width: 100%; display:block;">
                    <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.OfferingInstructorWrapper" />
                    <property name="propertyName" value="instructors" />
                    <property name="layoutManager.renderSequenceField" value="false" />
                    <property name="layoutManager.separateAddLine" value="false" />
                    <property name="layoutManager.richTable.render" value="false" />
                    <property name="layoutManager.applyDefaultCellWidths" value="false" />
                    <property name="layoutManager.applyAlternatingRowStyles" value="false" />
                    <property name="addLineItems">
                        <list>
                            <bean parent="Uif-InputField" p:propertyName="offeringInstructorInfo.personId" p:label="ID" p:required="true" p:cssClasses="col_5">
                                <property name="quickfinder.dataObjectClassName" value="org.kuali.rice.kim.api.identity.Person"/>
                                <property name="quickfinder.fieldConversions">
                                    <map>
                                        <entry key="name" value="offeringInstructorInfo.personName"/>
                                        <entry key="principalId" value="offeringInstructorInfo.personId"/>
                                    </map>
                                </property>
                                <property name="quickfinder.viewName" value="ks_simplePersonLookup" />
                                <property name="quickfinder.lightBoxLookup">
                                    <bean parent="Uif-LightBoxPost" p:lookupReturnByScript="true"/>
                                </property>
                            </bean>
                            <bean parent="Uif-DataField" p:propertyName="offeringInstructorInfo.personName" p:label="Name" p:readOnly="true"/>
                            <bean parent="Uif-InputField" p:propertyName="offeringInstructorInfo.typeKey" p:label="AFFILIATION">
                                <property name="control">
                                    <bean parent="Uif-DropdownControl" p:options-ref="aoInstructors-options" />
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:propertyName="sEffort" p:label="INST.EFFORT" p:required="true" p:exclusiveMin="0" p:inclusiveMax="100">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="10" />
                                </property>
                            </bean>
                        </list>
                    </property>
                    <property name="items">
                        <list>
                            <bean parent="Uif-DataField" p:propertyName="offeringInstructorInfo.personId" p:label="ID" p:readOnly="true"/>
                            <bean parent="Uif-DataField" p:propertyName="offeringInstructorInfo.personName" p:label="Name" p:readOnly="true"/>
                            <bean parent="Uif-InputField" p:propertyName="offeringInstructorInfo.typeKey" p:label="AFFILIATION" p:readOnly="true">
                                <property name="control">
                                    <bean parent="Uif-DropdownControl" p:options-ref="aoInstructors-options" />
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:propertyName="sEffort" p:label="INST.EFFORT" p:required="true" p:exclusiveMin="0" p:inclusiveMax="100">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="10" />
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-SeatPool" parent="Uif-VerticalBoxSection">
        <property name="cssClasses">
            <list merge="true">
                <value>ks-seatpools</value>
            </list>
        </property>
        <property name="componentSecurity.editAuthz" value="true"/>
        <property name="items">
            <list>
                <bean id="ActivityOffering-SeatpoolsSection" parent="Uif-TableCollectionSection"
                      p:layoutManager.numberOfColumns="6"
                      p:id="ao-seatpoolgroupZ"
                      p:layoutManager.style="width: 100%; display:block;"
                      p:layoutManager.id="ao-seatpoolgroup-table">
                    <property name="headerText" value="Seat Pools"/>
                    <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.SeatPoolWrapper" />
                    <property name="propertyName" value="seatpools" />
                    <property name="layoutManager.renderSequenceField" value="false" />
                    <property name="layoutManager.separateAddLine" value="false" />
                    <!-- KSENROLL-6911 fix, may relate to a bug in krad -->
                    <property name="layoutManager.richTable.render" value="true" />
                    <property name="layoutManager.applyDefaultCellWidths" value="false" />
                    <property name="layoutManager.applyAlternatingRowStyles" value="false" />
                    <property name="renderAddBlankLineButton" value="true" />
                    <property name="addLinePlacement" value="BOTTOM" />


                    <property name="items">
                        <list>
                            <bean parent="Uif-InputField" p:id="seatPoolPriority" p:propertyName="seatPool.processingPriority" p:label="PRIORITY" p:maxLength="2" p:required="true">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="2" />
                                </property>
                                <property name="validCharactersConstraint">
                                    <bean parent="IntegerPatternConstraint" />
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:propertyName="seatPool.seatLimit" p:label="SEATS" p:maxLength="3" p:id="seatLimit" >
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="3" p:onBlurScript="validateSeatsForSP(jQuery(this))" />
                                </property>
                                <property name="dependencyConstraints">
                                    <list>
                                        <bean parent="PrerequisiteConstraint" p:propertyName="seatPoolPopulation.name"/>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-MessageField" p:messageText="%" p:label="PERCENT" p:readOnly="true" p:id="seatLimitPercent" />
                            <!-- validatePopulationForSP('@{document.newMaintainableObject.dataObject.populationsJSONString}') -->
                            <bean parent="Uif-InputField" p:label="POPULATION NAME" p:id="ao-seatpoolgroup-population-name" p:propertyName="seatPoolPopulation.name"  p:cssClasses="col_6">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="40" p:onBlurScript="validatePopulationForSP(this,'@{document.newMaintainableObject.dataObject.populationsJSONString}');"/>
                                </property>
                                <property name="quickfinder.dataObjectClassName" value="org.kuali.student.enrollment.class2.population.dto.PopulationWrapper"/>
                                <property name="quickfinder.fieldConversions">
                                    <map>
                                        <entry key="populationInfo.id" value="seatPoolPopulation.id"/>
                                        <entry key="populationInfo.name" value="seatPoolPopulation.name"/>
                                    </map>
                                </property>
                                <property name="dependencyConstraints">
                                    <list>
                                        <bean parent="PrerequisiteConstraint" p:propertyName="seatPool.seatLimit"/>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:propertyName="seatPool.expirationMilestoneTypeKey" p:label="EXPIRATION MILESTONE">
                                <property name="control">
                                    <bean parent="Uif-DropdownControl" />
                                </property>
                                <property name="optionsFinder">
                                    <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.SeatPoolExpirationMilestoneTypeKeyValues" p:addBlankOption="false"/>
                                </property>
                            </bean>
                        </list>
                    </property>

                    <property name="lineActions">
                        <list>
                            <bean parent="Uif-SecondaryActionButton-Small" p:methodToCall="deleteLine" p:actionLabel="delete"
                                  p:actionEvent="deleteLine" p:onDocumentReadyScript="calculatePercent()" />
                        </list>
                    </property>
                    <property name="addBlankLineAction">
                        <bean parent="Uif-SecondaryActionButton-Small" p:onDocumentReadyScript="showSPAddlinePriority();" p:hidden="@{#parent.readOnly}">
                            <property name="methodToCall" value="addBlankLine"/>
                            <property name="actionEvent" value="addBlankLine"/>
                            <property name="id" value="uif-addBlankLine"/>
                            <property name="actionLabel" value="Add Another Seat Pool"/>
                            <property name="actionImagePlacement" value="LEFT"/>
                            <property name="actionImage">
                                <bean parent="Uif-Image" p:source="../ks-common-ksink/images/plus_15px.png"/>
                            </property>
                            <property name="style" value="margin-top:30px; margin-left:-150px; margin-bottom:15px"/>
                        </bean>
                    </property>

                </bean>
                <bean parent="Uif-MessageField" p:label="Seat Pools" p:messageText="0" p:id="seatpoolCount">
                </bean>
                <bean parent="Uif-MessageField" p:label="Seats Remaining" p:messageText="N/A" p:id="seatsRemaining">
                </bean>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-Miscellaneous" parent="Uif-VerticalBoxSection" p:headerText="Miscellaneous" p:id="aoInfo.Miscellaneous">
        <property name="items">
            <list>
                <bean id="course_url" parent="Uif-InputField-LabelTop"
                      p:propertyName="aoInfo.activityOfferingURL"
                      p:label="Course URL">
                    <property name="control">
                        <bean parent="Uif-TextControl"
                              p:size="30"/>
                    </property>
                </bean>
                <bean parent="Uif-VerticalFieldGroup"
                      p:label="Evaluation"
                      p:fieldLabel.renderColon="false"
                      p:style="margin-bottom:0">
                    <property name="items">
                        <list>
                            <bean id="is_evaluated"
                                  parent="Uif-InputField-LabelRight"
                                  p:propertyName="aoInfo.isEvaluated"
                                  p:label="This lecture requires an evaluation"
                                  p:fieldLabel.cssClasses="label-normal"
                                  p:required="false">
                                <property name="control">
                                    <bean parent="Uif-CheckboxControl"/>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <bean parent="Uif-VerticalFieldGroup"
