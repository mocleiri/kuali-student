    <bean id="KS-ManageRegistrationGroups-View" parent="KS-Uif-FormView-CustomizedBreadCrumb" p:applyDirtyCheck="true"
          p:id="manageRegistrationGroupsView"
          p:viewHelperServiceClass="org.kuali.student.enrollment.class2.courseoffering.service.impl.RegistrationGroupManagementViewHelperServiceImpl"
          p:formClass="org.kuali.student.enrollment.class2.courseoffering.form.RegistrationGroupManagementForm">

        <!--<property name="breadCrumbConfigMap">-->
            <!--<map>-->
                <!--<entry key="manageRegistrationGroupsPage">-->
                    <!--<map key-type="java.lang.String">-->
                        <!--<entry key="Home" value=""/>-->
                        <!--<entry key="Manage Course Offerings" value="courseOfferingManagementView:manageCourseOfferingsPage"/>-->
                        <!--<entry key="@{theCourseOffering.courseOfferingCode}" value=""/>-->
                    <!--</map>-->
                <!--</entry>-->
            <!--</map>-->
        <!--</property>-->

        <property name="authorizer">
            <bean class="org.kuali.student.enrollment.main.view.KsViewAuthorizerBase"/>
        </property>
        <property name="header">
            <bean parent="Uif-HeaderThree" p:headerText="Manage Registration Groups" />
        </property>
        <property name="footer">
            <bean parent="Uif-FooterBase"/>
        </property>
        <property name="singlePageView" value="true"/>
        <property name="page">
            <bean parent="Uif-Page" p:id="manageRegistrationGroupsPage" p:onDocumentReadyScript="replaceBreadCrumbs('@{breadCrumbJSON}');"/>
        </property>
        <property name="additionalScriptFiles">
            <list merge="true">
                <value>ks-enroll/scripts/common.js</value>
            </list>
        </property>
        <property name="additionalCssFiles">
            <list merge="true">
                <value>ks-enroll/css/acal.css</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-VerticalBoxSection">
                    <property name="headerText" value="@{theCourseOffering.courseOfferingCode} - @{theCourseOffering.courseOfferingTitle} (@{theCourseOffering.creditCnt} Credits)" />
                    <property name="disclosure.render" value="false"/>
                    <property name="header.cssClasses">
                        <list merge="true">
                            <value>ks-borderBottom</value>
                        </list>
                    </property>
                    <property name="items">
                        <list>
                            <bean parent="Uif-InputField" p:propertyName="formatOfferingIdForViewRG" p:label="Select Format" p:labelPlacement="LEFT" p:required="true" >
                                <property name="control">
                                    <bean parent="Uif-DropdownControl"/>
                                </property>
                                <property name="optionsFinder">
                                    <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.FormatsForCreateRGKeyValues" p:addBlankOption="false" />
                                </property>
                            </bean>
                            <bean parent="Uif-VerticalBoxGroup" p:refreshWhenChangedPropertyNames="formatOfferingIdForViewRG" p:methodToCallOnRefresh="filterAOsAndRGsPerFO">
                                <property name="items">
                                    <list>
                                        <ref bean="KS-ManageRegistrationGroups-UnassignedActivityOfferingsPerFormatSection-WithDisclosure"/>
                                        <ref bean="KS-ManageRegistrationGroups-UnassignedActivityOfferingsPerFormatSection"/>
                                        <ref bean="KS-ManageRegistrationGroups-ClusterForFormat"/>
                                        <ref bean="KS-ManageRegistrationGroups-NoClusterCondition"/>
                                        <ref bean="KS-ManageRegistrationGroups-HasClusterCondition"/>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-PrimaryActionButton-Small" p:methodToCall="generateAllRegGroups" p:actionLabel="Generate All Registration Groups" p:id="generate_all_button" p:render="@{hasAOCluster}"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
        <property name="dialogs">
            <list>
                <bean id="createNewClusterDialog"  parent="KS-ManageRegistrationGroups-CreateNewClusterDialog"/>
                <bean id="confirmToDeleteClusterDialog" parent="KS-ManageRegistrationGroups-ConfirmToDeleteClusterDialog" />
                <bean id="renameClusterDialog" parent="KS-ManageRegistrationGroups-RenameClusterDialog" />
            </list>
        </property>

    </bean>

    <bean id="KS-ManageRegistrationGroups-UnassignedActivityOfferingsPerFormatSection-WithDisclosure" parent="Uif-Disclosure-TableCollectionSection"
          p:renderAddLine="false"  p:renderLineActions="false" p:render="@{!hasAOCluster}">
        <!--<property name="disclosure.render" value="true"/>-->
        <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingWrapper" />
        <property name="propertyName" value="filteredUnassignedAOsForSelectedFO" />
        <property name="headerText" value="@{formatOfferingName} Unassigned Activity Offerings"/>
        <property name="collectionLookup.quickfinderAction.render" value="false" />
        <property name="layoutManager.renderSequenceField" value="false" />
        <property name="layoutManager.applyDefaultCellWidths" value="false" />
        <property name="layoutManager.applyAlternatingRowStyles" value="false" />
        <property name="layoutManager.richTable.templateOptions">
            <map merge="true">
                <entry key="sDom" value="rtip"/>
                <entry key="aaSorting" value="[[ 2, 'asc' ], [0, 'asc']]"/>
                <entry key="bPaginate" value="false"/>
                <entry key="aoColumns"
                       value="[ {'sWidth':'4%'},
                                {'sWidth':'6%'},
                                {'sWidth':'10%'},
                                {'sWidth':'6%'},
                                {'sWidth':'8%'},
                                {'sWidth':'8%'},
                                {'sWidth':'8%'},
                                {'sWidth':'21%'},
                                {'sWidth':'5%'},
                                {'sWidth':'30%'}
                           ]"/>
            </map>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-DataField"
                      p:label="CODE"
                      p:propertyName="aoInfo.activityCode">
                </bean>
                <bean parent="Uif-DataField"
                      p:label="STATUS"
                      p:propertyName="stateName" />
                <bean parent="Uif-DataField"
                      p:label="TYPE"
                      p:propertyName="typeName"/>
                <bean parent="Uif-DataField"
                      p:label="MAX ENR"
                      p:propertyName="aoInfo.maximumEnrollment"/>
                <!--<bean parent="Uif-DataField"
                      p:label="DAYS"
                      p:propertyName="daysDisplayName"/>-->
                <bean parent="Uif-MessageField"
                      p:label="DAYS"
                      p:messageText="@{#line.daysDisplayName}" />
                <!--<bean parent="Uif-DataField"
                      p:label="START TIME"
                      p:propertyName="startTimeDisplay"/>-->
                <bean parent="Uif-MessageField"
                      p:label="START TIME"
                      p:messageText="@{#line.startTimeDisplay}" />

                <!--<bean parent="Uif-DataField"
                      p:label="END TIME"
                      p:propertyName="endTimeDisplay"/>-->
                <bean parent="Uif-MessageField"
                      p:label="END TIME"
                      p:messageText="@{#line.endTimeDisplay}" />

                <!--<bean parent="Uif-DataField"
                      p:label="BUILDING"
                      p:propertyName="buildingName"/>-->
                <bean parent="Uif-MessageField"
                      p:label="BUILDING"
                      p:messageText="@{#line.buildingName}" />

                <!--<bean parent="Uif-DataField"
                      p:label="ROOM"
                      p:propertyName="roomName"/>-->
                <bean parent="Uif-MessageField"
                      p:label="ROOM"
                      p:messageText="@{#line.roomName}" />

                <bean parent="Uif-MessageField"
                      p:label="INSTRUCTOR"
                      p:messageText="@{#line.instructorDisplayNames}" />
            </list>
        </property>
    </bean>

    <bean id="KS-ManageRegistrationGroups-UnassignedActivityOfferingsPerFormatSection" parent="Uif-TableCollectionSection"
          p:renderAddLine="false"  p:renderLineActions="false" p:render="@{hasAOCluster}">
        <!--<property name="disclosure.render" value="true"/>-->
        <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingWrapper" />
        <property name="propertyName" value="filteredUnassignedAOsForSelectedFO" />
        <property name="headerText" value="@{formatOfferingName} Unassigned Activity Offerings"/>
        <property name="collectionLookup.quickfinderAction.render" value="false" />
        <property name="layoutManager.renderSequenceField" value="false" />
        <property name="layoutManager.applyDefaultCellWidths" value="false" />
        <property name="layoutManager.applyAlternatingRowStyles" value="false" />
        <property name="layoutManager.richTable.templateOptions">
            <map merge="true">
                <entry key="sDom" value="rtip"/>
                <entry key="aaSorting" value="[[ 3, 'asc' ], [1, 'asc']]"/>
                <entry key="bPaginate" value="false"/>
                <entry key="bAutoWidth" value="false"/>
            </map>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-InputField" p:propertyName="isChecked" p:required="false" p:onChangeScript="toggleAssignButton();">
                    <property name="control">
                        <bean parent="Uif-CheckboxControl" p:disabled="false"/>
                    </property>
                </bean>
                <bean parent="Uif-DataField"
                      p:label="CODE"
                      p:propertyName="aoInfo.activityCode">
                </bean>
                <bean parent="Uif-DataField"
                      p:label="STATUS"
                      p:propertyName="stateName" />
                <bean parent="Uif-DataField"
                      p:label="TYPE"
                      p:propertyName="typeName"/>
                <bean parent="Uif-DataField"
                      p:label="MAX ENR"
                      p:propertyName="aoInfo.maximumEnrollment"/>
                <!--<bean parent="Uif-DataField"
                      p:label="DAYS"
                      p:propertyName="daysDisplayName"/>-->
                <bean parent="Uif-MessageField"
                      p:label="DAYS"
                      p:messageText="@{#line.daysDisplayName}" />
                <!--<bean parent="Uif-DataField"
                      p:label="START TIME"
                      p:propertyName="startTimeDisplay"/>-->
                <bean parent="Uif-MessageField"
                      p:label="START TIME"
                      p:messageText="@{#line.startTimeDisplay}" />

                <!--<bean parent="Uif-DataField"
                      p:label="END TIME"
                      p:propertyName="endTimeDisplay"/>-->
                <bean parent="Uif-MessageField"
                      p:label="END TIME"
                      p:messageText="@{#line.endTimeDisplay}" />

                <!--<bean parent="Uif-DataField"
                      p:label="BUILDING"
                      p:propertyName="buildingName"/>-->
                <bean parent="Uif-MessageField"
                      p:label="BUILDING"
                      p:messageText="@{#line.buildingName}" />

                <!--<bean parent="Uif-DataField"
                      p:label="ROOM"
                      p:propertyName="roomName"/>-->
                <bean parent="Uif-MessageField"
                      p:label="ROOM"
                      p:messageText="@{#line.roomName}" />

                <bean parent="Uif-MessageField"
                      p:label="INSTRUCTOR"
                      p:messageText="@{#line.instructorDisplayNames}" />

                <!--<bean parent="Uif-DataField"
                      p:label="INSTRUCTOR"
                      p:propertyName="instructorDisplayNames"/>-->
            </list>
        </property>
    </bean>


    <bean id="KS-ManageRegistrationGroups-ClusterForFormat" parent="Uif-HorizontalBoxSection" p:render="@{hasAOCluster}" p:onDocumentReadyScript="addDropdownGroup('KS-ManageRegistrationGroups-ClusterForFormat_Dropdown_control', false, '-----------------', {'create-new' : 'Create new ...'}, @{withinPortal} );">

        <property name="items">
            <list>
                <bean id="KS-ManageRegistrationGroups-ClusterForFormat_Dropdown" parent="Uif-InputField" p:propertyName="clusterIdIdForNewFO"  p:label="Assign selected offerings to" p:labelPlacement="TOP" >
                    <property name="control">
                        <bean parent="Uif-DropdownControl"/>
                    </property>
                    <property name="optionsFinder">
                        <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.ListOfAOClustersForFOKeyValues" p:addBlankOption="false" />
                    </property>
                </bean>
                <bean parent="Uif-SecondaryActionButton-Small" p:actionLabel="Assign" p:methodToCall="moveAOToACluster"  p:id="move_ao_button" p:style="margin-top: 18px;" p:successCallback="renameDialogButtons({'Y':'Create Cluster', 'N': 'Cancel'});"/>
            </list>
        </property>
    </bean>

    <bean id="KS-ManageRegistrationGroups-NoClusterCondition" parent="Uif-VerticalBoxSection" p:render="@{!hasAOCluster}">
        <property name="id" value="noClusterCondition"/>
        <property name="disclosure.render" value="false"/>
        <property name="items">
            <list>
                <bean parent="Uif-VerticalBoxSection">
                    <property name="disclosure.render" value="false"/>
                    <property name="headerText" value="Generate registration groups"/>
                    <property name="items">
                        <list>
                            <bean parent="Uif-Message" p:messageText="Unconstrained registration groups for this course offering are listed below.  Generate unconstrained registration groups if additional activities have been added or removed since the groups were last generated." />
                            <bean parent="Uif-PrimaryActionButton-Small" p:actionLabel="Generate Registration Groups" p:methodToCall="generateUnconstrainedRegGroups"
                                  p:id="generate_unconstrained_rgs_button"/>
                        </list>
                    </property>
                </bean>
                <bean parent="Uif-VerticalBoxSection">
                    <property name="disclosure.render" value="false"/>
                    <property name="headerText" value="or create an activity offering cluster to manage constrained registration groups"/>
                    <property name="items">
                        <list>
                            <bean parent="Uif-SecondaryActionButton-Small" p:actionLabel="Create New Cluster" p:methodToCall="createNewClusterFromLightBox"
                                  p:id="create_new_cluster_button" p:render="@{!withinPortal}"/>
                            <ref bean="KS-ManageRegistrationGroups-CreateNewClusterSection"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="KS-ManageRegistrationGroups-HasClusterCondition" parent="Uif-VerticalBoxSection" p:render="@{hasAOCluster}">
        <property name="id" value="hasClusterCondition"/>
        <property name="headerText" value="Activity Offering Clusters" />
        <property name="disclosure.render" value="false"/>
        <property name="header.cssClasses">
            <list merge="true">
                <value>ks-borderBottom</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-SecondaryActionButton-Small" p:actionLabel="Create New Cluster" p:methodToCall="createNewClusterFromLightBox"
                      p:id="create_new_cluster_button" p:render="@{!withinPortal}" p:successCallback="renameDialogButtons({'Y':'Create Cluster', 'N': 'Cancel'});"/>
                <ref bean="KS-ManageRegistrationGroups-CreateNewClusterSection"/>
                <ref bean="KS-ManageRegistrationGroups-ClusterCollection"/>
            </list>
        </property>
    </bean>

    <bean id="KS-ManageRegistrationGroups-ClusterHeaderText" parent="Uif-HorizontalFieldGroup" p:fieldLabel.render="false" p:style="display: none">
        <property name="items">
            <list>
                <bean parent="Uif-Label" p:labelText="@{#line.clusterNameForDisplay}" p:renderColon="false">
                </bean>
            </list>
        </property>
    </bean>

    <bean id="KS-ManageRegistrationGroups-LightboxPropsText" parent="Uif-HorizontalFieldGroup" p:fieldLabel.render="false" p:style="display: none">
        <property name="items">
            <list>
                <bean parent="Uif-Label" p:labelText="@{#line.aoCluster.privateName}" p:renderColon="false">
                    <property name="id"  value="privateName"/>
                </bean>
                <bean parent="Uif-Label" p:labelText="@{#line.aoCluster.Name}" p:renderColon="false">
                    <property name="id"  value="publishedName"/>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="KS-ManageRegistrationGroups-ClusterCollection" parent="KS-Uif-StackedCollectionSection-HorizontalSubCollectionFieldGroup"
          p:renderAddLine="false"  p:renderLineActions="false" p:onDocumentReadyScript="removeSelfFromDropdowns('KS-ManageRegistrationGroups-ClusterHeaderText');">
        <property name="disclosure.render" value="false"/>
        <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingClusterWrapper" />
        <property name="propertyName" value="filteredAOClusterWrapperList" />
        <property name="cssClasses" value="aoc_disclosure"/>
        <property name="headerText" value=""/>
        <property name="collectionLookup.quickfinderAction.render" value="false" />
        <property name="layoutManager.lineGroupPrototype.disclosure.render" value="true"/>
        <property name="layoutManager.lineGroupPrototype.header.headerText" value="@{#line.clusterNameForDisplay}"/>
        <property name="layoutManager.addLineGroup.header.render" value="false"/>

        <property name="items">
            <list>
                <ref bean="KS-ManageRegistrationGroups-LightboxPropsText"/>
                <ref bean="KS-ManageRegistrationGroups-ClusterHeaderText"/>
                <ref bean="KS-ManageRegistrationGroups-StateAndActionLinks"/>
            </list>
        </property>
        <property name="subCollections">
            <list>
                <ref bean="KS-ManageRegistrationGroups-ActivityOfferingsPerClusterSubSection" />
                <ref bean="KS-ManageRegistrationGroups-RegistrationGroupsPerClusterSubSection" />
            </list>
        </property>
    </bean>

    <bean id="KS-ManageRegistrationGroups-CreateNewClusterSection" parent="Uif-HorizontalBoxSection"
          p:render="@{withinPortal}">
        <property name="id" value="createNewClusterSection" />
        <property name="disclosure.render" value="false"/>
        <property name="headerText" value=""/>
        <property name="items">
            <list>
                <bean parent="Uif-InputField"  p:propertyName="privateClusterName" p:label="Private Name" p:labelPlacement="TOP" p:required="true" p:render="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="40" p:minLength="3" p:maxLength="40"/>
                    </property>
                </bean>
                <bean parent="Uif-InputField"  p:propertyName="publishedClusterName" p:label="Published Name" p:labelPlacement="TOP" p:render="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="40" p:minLength="3" p:maxLength="40"/>
                    </property>
                </bean>

                <bean parent="Uif-SecondaryActionButton-Small" p:performClientSideValidation="true" p:actionLabel="Create Cluster" p:methodToCall="createNewCluster"  p:style="margin-top:18px;"/>

            </list>
        </property>
    </bean>

    <bean id="KS-ManageRegistrationGroups-StateAndActionLinks" parent="Uif-HorizontalFieldGroup" p:fieldLabel.render="false" p:cssClasses="aoc_stateandlinks">
        <property name="items">
            <list>
                <bean parent="Uif-Label" p:labelText="@{#line.rgStatus}" p:renderColon="false">
                    <property name="cssClasses">
                        <list merge="true">
                            <value>@{#line.rgMessageStyle}</value>
                        </list>
                    </property>
                </bean>
                <bean parent="Uif-LinkGroup" p:style="float:right; padding: 5px;">
                    <property name="items">
                        <list>
                            <bean parent="Uif-ActionLink" p:methodToCall="viewRegGroupsPerCluster" p:actionLabel="View Registration Groups" p:render="@{#line.hasAllRegGroups}"
                                  p:onClickScript="writeHiddenToForm(&quot;actionParameters[selectedLineIndex]&quot;, &quot;@{#index}&quot;);" p:actionScript = "showLightbox('registrationGroupsPerCluster_line@{#index}', '@{#ConfigProperties['application.url']}');"/>
                            <bean parent="Uif-ActionLink" p:methodToCall="generateRegGroupsPerCluster" p:actionLabel="Generate Registration Groups" p:render="@{!#line.hasAllRegGroups}" p:onClickScript="writeHiddenToForm(&quot;actionParameters[selectedLineIndex]&quot;, &quot;@{#index}&quot;);"/>
                            <bean parent="Uif-Label" p:labelText="|" p:renderColon="false" />
                            <bean parent="Uif-ActionLink" p:methodToCall="renameAClusterThroughDialog" p:actionLabel="Rename" p:render="@{!withinPortal}" p:onClickScript="writeHiddenToForm(&quot;actionParameters[selectedLineIndex]&quot;, &quot;@{#index}&quot;);" p:successCallback="populateLightboxForm('KS-ManageRegistrationGroups-ClusterHeaderText', {'privateClusterNameForRename' : 'privateName_line@{#index}', 'publishedClusterNameForRename' : 'publishedName_line@{#index}'}); renameDialogButtons({'Y':'Replace', 'N': 'Cancel'});"/>
                            <bean parent="Uif-Label" p:labelText="|" p:renderColon="false" p:render="@{!withinPortal}"/>
                            <bean parent="Uif-ActionLink" p:methodToCall="deleteACluster" p:actionLabel="Delete" p:render="@{withinPortal}"
                                  p:preSubmitCall="return confirm('This action is not reversible. Are you sure you want to continue?')"
                                  p:onClickScript="writeHiddenToForm(&quot;actionParameters[selectedLineIndex]&quot;, &quot;@{#index}&quot;);"/>
                            <bean parent="Uif-ActionLink" p:methodToCall="deleteAClusterThroughDialog" p:actionLabel="Delete" p:render="@{!withinPortal}"
                                  p:onClickScript="writeHiddenToForm(&quot;actionParameters[selectedLineIndex]&quot;, &quot;@{#index}&quot;);" />
                            <!--p:actionScript="showLightboxComponent('confirmToDeleteClusterDialog');"  />-->
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="KS-ManageRegistrationGroups-ActivityOfferingsPerClusterSubSection" parent="Uif-TableCollectionSubSection"
          p:renderAddLine="false"  p:renderLineActions="true">
        <property name="id" value="activityOfferingsPerCluster" />
        <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingWrapper" />
        <property name="propertyName" value="aoWrapperList" />
        <property name="headerText" value=""/>
        <property name="collectionLookup.quickfinderAction.render" value="false" />
        <property name="layoutManager.renderSequenceField" value="false" />
        <property name="layoutManager.applyDefaultCellWidths" value="false" />
        <property name="layoutManager.applyAlternatingRowStyles" value="false" />
        <property name="layoutManager.richTable.templateOptions">
            <map merge="true">
                <entry key="sDom" value="rtip"/>
                <entry key="aaSorting" value="[[ 2, 'asc' ], [0, 'asc']]"/>
                <entry key="bPaginate" value="false"/>
                <entry key="bAutoWidth" value="false"/>
            </map>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-InputField" p:propertyName="isChecked" p:required="false" >
                    <property name="control">
                        <bean parent="Uif-CheckboxControl" p:disabled="false"/>
                    </property>
                </bean>
                <bean parent="Uif-DataField"
                      p:label="CODE"
                      p:propertyName="aoInfo.activityCode">
                </bean>
                <bean parent="Uif-DataField"
                      p:label="STATUS"
                      p:propertyName="stateName" />
                <bean parent="Uif-DataField"
                      p:label="TYPE"
                      p:propertyName="typeName"/>
                <bean parent="Uif-DataField"
                      p:label="MAX ENR"
                      p:propertyName="aoInfo.maximumEnrollment"/>
                <!--<bean parent="Uif-DataField"
                      p:label="DAYS"
                      p:propertyName="daysDisplayName"/>-->
                <bean parent="Uif-MessageField"
                      p:label="DAYS"
                      p:messageText="@{#line.daysDisplayName}" />
                <!--<bean parent="Uif-MessageField"
                      p:label="START TIME"
                      p:propertyName="startTimeDisplay"/>-->

                <bean parent="Uif-MessageField"
                      p:label="START TIME"
                      p:messageText="@{#line.startTimeDisplay}" />


                <!--<bean parent="Uif-DataField"
                      p:label="END TIME"
                      p:propertyName="endTimeDisplay" >
                </bean>-->
                <bean parent="Uif-MessageField"
                      p:label="END TIME"
                      p:messageText="@{#line.endTimeDisplay}" />

                <!--<bean parent="Uif-DataField"
                      p:label="BUILDING"
                      p:propertyName="buildingName"/>-->
                <bean parent="Uif-MessageField"
                      p:label="BUILDING"
                      p:messageText="@{#line.buildingName}" />

                <!--<bean parent="Uif-DataField"
                      p:label="ROOM"
                      p:propertyName="roomName"/>-->
                <bean parent="Uif-MessageField"
                      p:label="ROOM"
                      p:messageText="@{#line.roomName}" />

                <bean parent="Uif-MessageField"
                      p:label="INSTRUCTOR"
                      p:messageText="@{#line.instructorDisplayNames}" />
            </list>
        </property>

        <property name="footer">
            <bean parent="Uif-FooterBase">
                <property name="items">
                    <list>
                        <bean id="KS-ManageRegistrationGroups-MoveAOBetweenClusters" parent="Uif-InputField" p:propertyName="clusterIdForAOMove" p:label="Move selected offerings to" p:labelPlacement="TOP">
                            <property name="control">
                                <bean parent="Uif-DropdownControl" />
                            </property>
                            <property name="optionsFinder">
                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.ListOfAOClustersForFOKeyValues" p:addBlankOption="false"/>
