    <bean id="KS-TestServiceCall-FirstServicePage" parent="KS-Uif-Page" p:disclosure.render="false"
        p:id="firstServiceCall"
        p:headerText="Test GetSocIdsByTerm">

        <!-- It's easier to put the footer up front since it has a lot less content -->
        <property name="footer"> <!-- Start of footer-->
            <bean parent="Uif-FooterBase">
                <property name="items">
                    <list>
                        <bean parent="KS-Uif-PrimaryActionButton" p:performClientSideValidation="true" p:actionLabel="Test Service Call"
