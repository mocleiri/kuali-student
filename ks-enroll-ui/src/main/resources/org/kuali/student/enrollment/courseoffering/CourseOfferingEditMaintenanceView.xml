    <bean id="KS-CourseOfferingEdit-DeliveryFormats" parent="KS-CourseOffering-DeliveryFormats-parent"/>
    <bean id="KS-CourseOffering-DeliveryFormats-parent" parent="Uif-HorizontalBoxSubSection" abstract="true">


        <property name="instructionalText" value="Add the delivery formats to be used for this course offering"/>
        <property name="refreshWhenChangedPropertyNames" value="#dp.courseOfferingInfo.finalExamType"/>

        <property name="header.headerText" value="Delivery Formats"/>
        <property name="header.headerLevel" value="Uif-HeaderOne"/>
        <property name="header.style" value="font-weight:bold;"/>

        <property name="items">
