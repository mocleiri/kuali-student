    <bean id="CourseOfferingEditWrapper" parent="CourseOfferingEditWrapper-parentBean"/>

    <bean id="CourseOfferingEditWrapper-parentBean" abstract="true" parent="DataObjectEntry">
        <property name="dataObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.CourseOfferingEditWrapper"/>
        <property name="titleAttribute" value="id"/>
        <property name="attributes">
            <list>
            </list>
        </property>
        <property name="objectLabel" value="Course Offering"/>
        <property name="primaryKeys">
            <list>
                <value>courseOfferingInfo.id</value>
            </list>
        </property>
    </bean>

    <!-- InquiryView -->
    <bean id="KS-CourseOfferingEditWrapper-InquiryView" parent="KS-Uif-InquiryView"
          p:headerText="Course Offering Inquiry"
          p:dataObjectClassName="org.kuali.student.enrollment.class2.courseoffering.dto.CourseOfferingEditWrapper"
          p:viewHelperServiceClass="org.kuali.student.enrollment.class2.courseoffering.service.impl.CourseOfferingEditInquirableImpl"
            >


        <property name="page.header">
            <!-- suppress Expand/Collapse buttons -->
            <ref bean="KS-Uif-HeaderOne-Empty" />
        </property>
        <property name="Items">
            <list>
                <bean parent="Uif-VerticalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="Uif-GridGroup"
                                  p:layoutManager.numberOfColumns="2"
                                  p:layoutManager.applyDefaultCellWidths="false">
                                <property name="items">
                                    <list>
                                        <!--<bean parent="Uif-DataField" p:label="ID" p:propertyName="courseOfferingInfo.id" />-->
                                        <bean parent="Uif-DataField" p:propertyName="courseOfferingInfo.courseOfferingCode" />
                                        <bean parent="Uif-DataField" p:label="Cross listed as" p:propertyName="alternateCOCodesUIList" p:render="@{dataObject.alternateCOCodesUIList != ''}"/>
                                        <bean parent="Uif-DataField" p:propertyName="courseOfferingInfo.courseNumberSuffix" />
                                        <bean parent="Uif-DataField" p:propertyName="courseOfferingInfo.courseOfferingTitle" />
                                        <bean parent="Uif-DataField" p:propertyName="termName" p:label="Term"/>
                                        <bean parent="Uif-DataField" p:propertyName="courseOfferingInfo.creditCnt" />

                                        <!--<bean parent="Uif-DataField" p:label="Grading Option" p:propertyName="selectedGradingOptionName" />-->
                                        <bean parent="Uif-InputField" p:propertyName="courseOfferingInfo.gradingOptionId" p:id="gradingOptionId" p:label="Grading Options" p:componentSecurity.editAuthz="true">
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.GradingOptionsKeyValues" />
                                            </property>
                                        </bean>

                                        <!-- displays "[]" when empty, not desired
                                        <bean parent="Uif-InputField" p:propertyName="courseOfferingInfo.studentRegistrationOptionIds" p:label="Student Registration Options" p:render="@{#dp.studentRegOptions.empty}">
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.StudentRegistrationOptionsKeyValues" p:addBlankOption="false"/>
                                            </property>
                                        </bean>
                                        -->
                                        <bean parent="Uif-DataField" p:propertyName="selectedStudentRegOpts" p:label="Student Registration Options" />
                                        <bean parent="Uif-DataField" p:propertyName="courseOfferingInfo.finalExamType" />
                                        <bean parent="Uif-MessageField" p:label="Waited Listed" p:messageText="@{(dataObject.courseOfferingInfo.hasWaitlist eq null or !dataObject.courseOfferingInfo.hasWaitlist) ? 'NO':'YES'}"/>

                                        <bean parent="Uif-InputField" p:propertyName="courseOfferingInfo.waitlistLevelTypeKey" p:label="Waitlist Level" p:inquiry.render="false" p:fieldLabel.renderColon="true" p:labelPlacement="LEFT" p:render="@{!(dataObject.courseOfferingInfo.waitlistLevelTypeKey eq null) and (dataObject.courseOfferingInfo.hasWaitlist)}" >
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.WaitlistLevelOptionsKeyValues"/>
                                            </property>
                                        </bean>
                                        <bean parent="Uif-InputField" p:propertyName="courseOfferingInfo.waitlistTypeKey" p:label="Waitlist Type" p:inquiry.render="false" p:fieldLabel.renderColon="true" p:labelPlacement="LEFT" p:render="@{!(dataObject.courseOfferingInfo.waitlistTypeKey eq null) and (dataObject.courseOfferingInfo.hasWaitlist)}" >
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.WaitlistTypeOptionsKeyValues"/>
                                            </property>
                                        </bean>

                                        <bean parent="Uif-MessageField" p:label="Honors Flag" p:messageText="@{(dataObject.courseOfferingInfo.isHonorsOffering eq null or !dataObject.courseOfferingInfo.isHonorsOffering) ? 'NO':'YES'}" />


                                    </list>
                                </property>
                            </bean>
                            <bean  parent="Uif-TableCollectionSection">
                                <property name="headerText" value="Delivery Formats" />
                                <property name="propertyName" value="formatOfferingList" />
                                <property name="collectionObjectClass" value="org.kuali.student.enrollment.courseoffering.dto.FormatOfferingInfo" />
                                <property name="layoutManager.renderSequenceField" value="false" />
                                <property name="layoutManager.separateAddLine" value="false" />
                                <property name="layoutManager.richTable.render" value="false" />
                                <property name="layoutManager.applyDefaultCellWidths" value="false" />
                                <property name="layoutManager.applyAlternatingRowStyles" value="false" />
                                <property name="items">
                                    <list>
                                        <bean parent="Uif-DataField" p:propertyName="Name" p:label="Format Name" />
                                        <bean parent="Uif-InputField" p:propertyName="gradeRosterLevelTypeKey"
                                              p:label="Grade Roster Level">
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.CourseOfferingTypeKeyValues"/>
                                            </property>
                                        </bean>
                                        <bean parent="Uif-InputField" p:propertyName="finalExamLevelTypeKey"
                                              p:label="Final Exam Driver" p:render="@{#dp.courseOfferingInfo.finalExamType eq 'STANDARD'}">
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.CourseOfferingTypeKeyValues"/>
                                            </property>
                                        </bean>
                                        <bean parent="Uif-MessageField" p:label="Final Exam Driver"
                                              p:messageText="Alternate exam for this offering"
                                              p:render="@{#dp.courseOfferingInfo.finalExamType eq 'ALTERNATE'}" />
                                        <bean parent="Uif-MessageField" p:label="Final Exam Driver"
                                              p:messageText="No final exam for this offering"
                                              p:render="@{#dp.courseOfferingInfo.finalExamType eq 'NONE'}" />
                                    </list>
                                </property>
                            </bean>

                            <bean  parent="Uif-TableCollectionSection">
                                <property name="headerText" value="Affiliated Personnel" />
                                <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.OfferingInstructorWrapper" />
                                <property name="propertyName" value="instructors" />
                                <property name="layoutManager.renderSequenceField" value="false" />
                                <property name="layoutManager.separateAddLine" value="false" />
                                <property name="layoutManager.richTable.render" value="false" />
                                <property name="layoutManager.applyDefaultCellWidths" value="false" />
                                <property name="layoutManager.applyAlternatingRowStyles" value="false" />
                                <property name="items">
                                    <list>
                                        <bean parent="Uif-DataField" p:label="Affiliated Personnel Name" p:propertyName="offeringInstructorInfo.personName" />
                                        <bean parent="Uif-DataField" p:label="Affiliated Personnel Affiliation" p:propertyName="typeName" />
                                    </list>
                                </property>
                            </bean>

                            <bean parent="Uif-TableCollectionSection">
                                <property name="propertyName" value="organizationNames"/>
                                <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.OrganizationInfoWrapper"/>
                                <property name="headerText" value="Administering Organizations"/>
                                <property name="layoutManager.renderSequenceField" value="false" />
                                <property name="layoutManager.separateAddLine" value="false" />
                                <property name="layoutManager.richTable.render" value="false" />
                                <property name="layoutManager.applyDefaultCellWidths" value="false" />
                                <property name="layoutManager.applyAlternatingRowStyles" value="false" />
                                <property name="items">
                                    <list>
                                        <bean parent="Uif-DataField" p:propertyName="organizationName" p:label="Organization Name"/>
                                    </list>
                                </property>
                            </bean>
                            <!-- end of Uif-GridGroup -->

                        </list>
                    </property>
                </bean>
                <!-- end of Uif-VerticalBoxSection -->

            </list>
        </property>
    </bean>

    <!-- Edit a Course Offering MaintenanceView -->
    <bean id="KS-CourseOfferingEditWrapper-EditMaintenanceView" parent="KS-Uif-MaintenanceView">
        <property name="authorizer">
            <bean class="org.kuali.student.enrollment.main.view.KsMaintenanceViewAuthorizerBase"/>
        </property>
        <property name="headerText" value="Edit @{document.oldMaintainableObject.dataObject.courseOfferingInfo.courseOfferingCode} - @{document.oldMaintainableObject.dataObject.courseOfferingInfo.courseOfferingTitle} (@{document.oldMaintainableObject.dataObject.courseOfferingInfo.creditCnt} Credits)"/>
        <property name="dataObjectClassName" value="org.kuali.student.enrollment.class2.courseoffering.dto.CourseOfferingEditWrapper"/>
        <!-- <property name="render" value="@{#hasPermDtls('KS-ENR','Open Views for KS-CourseOfferingEditWrapper-EditMaintenanceView',null,document.oldMaintainableObject.dataObject.adminOrg)}" /> -->

        <property name="breadcrumbOptions.homewardPathBreadcrumbs">
            <list>
                <bean parent="Uif-BreadcrumbItem">
                    <property name="label" value="Home"/>
                    <property name="url.href" value="@{#ConfigProperties['application.url']}/portal.do"/>
                </bean>
                <bean parent="Uif-BreadcrumbItem">
                    <property name="label" value="Enrollment"/>
                    <property name="url.viewId" value="enrollmentHomeView"/>
                </bean>
            </list>
        </property>

        <property name="header.lowerGroup">
            <bean parent="Uif-HeaderLowerGroup">
                <property name="style" value="margin-bottom:px;"/>
                <property name="items">
                    <list>
                        <bean parent="Uif-MessageField" p:messageText="Crosslisted as: @{#dp.alternateCOCodesUIList}" p:render="@{!#dp.alternateCourseCodesSuffixStripped.isEmpty()}" p:style="font-weight:bold; margin-top:4px; margin-bottom:8px;"/>
                    </list>
                </property>
            </bean>
        </property>
        <!--<property name="viewLabelFieldPropertyName" value="Edit @{document.oldMaintainableObject.dataObject.courseOfferingInfo.courseOfferingCode} - @{document.oldMaintainableObject.dataObject.courseOfferingInfo.courseOfferingTitle} (@{document.oldMaintainableObject.dataObject.courseOfferingInfo.creditCnt} Credits)"/>-->
        <!--<property name="breadcrumbTitleDisplayOption" value="replace"/>-->

        <property name="viewHelperServiceClass" value="org.kuali.student.enrollment.class2.courseoffering.service.impl.CourseOfferingEditMaintainableImpl" />
        <property name="page.items">
            <list>
                <ref bean="KS-EditCourseOffering-AliasMessageSection" />
                <ref bean="KS-CourseOfferingEdit-Term"/>
                <ref bean="KS-Uif-DocumentPageFooter-SubmitCancel"/>
                <ref bean="KS-CourseOfferingEdit-CourseOfferingInformation"/>
                <ref bean="KS-CourseOfferingEdit-DeliveryAndAssessment"/>
                <ref bean="KS-CourseOfferingEdit-Waitlist"/>
                <ref bean="KS-CourseOfferingEdit-PersonnelSection"/>
                <ref bean="KS-CourseOfferingEdit-MiscSection"/>

            </list>
        </property>
        <property name="page.footer.items">
            <ref bean="KS-Uif-DocumentPageFooter-SubmitCancel" />
        </property>
    </bean>

    <bean id="KS-EditCourseOffering-AliasMessageSection" parent="Uif-HorizontalBoxSection">
        <property name="render" value="@{document.newMaintainableObject.dataObject.editCrossListedCoAlias}"/>
        <property name="style" value="background-color:#DDECFF;padding-top: 5px; border: 1px solid #9CC4F9;"/>
        <property name="disclosure.render" value="false"/>
        <property name="items" >
            <list>
                <bean parent="Uif-ImageField" p:image.altText="information"
                      p:source="@{#ConfigProperties['ks.externalizable.images.url']}/information.png">
                    <property name="style" value="padding-right:10px;padding-left:10px"/>
                </bean>
                <bean id="KS-EditCourseOffering-AliasMessage" parent="Uif-MessageField"
                      p:messageText="@{document.newMaintainableObject.dataObject.alternateCOCodesUIList} is a crosslisted alias
                                            for: @{document.newMaintainableObject.dataObject.courseOfferingCode}(Owner). Changes made on
                                            this page to @{document.newMaintainableObject.dataObject.courseOfferingCode}(Owner) will be applied to all
                                            aliases, including @{document.newMaintainableObject.dataObject.alternateCOCodesUIList}." />
            </list>
        </property>
    </bean>

    <bean id="KS-CourseOfferingEdit-Term" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <bean parent="Uif-DataField" p:label="Term" p:propertyName="termStartEnd"/>
            </list>
        </property>
    </bean>

    <bean id="KS-CourseOfferingEdit-CourseOfferingInformation" parent="Uif-VerticalBoxSection" p:headerText="Course Offering Information">
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="&lt;hr&gt;"/>
                <bean parent="Uif-VerticalBoxSubSection">
                    <property name="items">
                        <list>
                            <bean parent="Uif-Label" p:labelText="Course Offering Code" p:renderColon="false"/>
                            <bean parent="Uif-HorizontalFieldGroup" p:showLabel="false">
                                <property name="items">
                                    <list>
                                        <bean parent="Uif-InputField" p:propertyName="course.code" p:showLabel="false"
                                              p:readOnly="true" />
                                        <bean parent="Uif-InputField" p:propertyName="courseOfferingInfo.courseNumberSuffix"  p:showLabel="false" p:fieldLabel.renderColon="false" p:labelPlacement="TOP">
                                            <property name="control">
                                                <bean parent="Uif-TextControl" p:watermarkText="Suffix" p:size="5" />
                                            </property>
                                        </bean>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <ref bean="KS-COEditListed-Checkbox-Group"/>
                <!--<ref bean="KS-COEditListed-CO-Codes-display"/>-->
                <ref bean="KS-GradingAndRegOptions"/>
            </list>
        </property>
    </bean>

    <bean id="KS-COEditListed-Checkbox-Group" parent="Uif-InputField"  p:propertyName="alternateCourseCodesSuffixStripped" p:readOnlyDisplayReplacementPropertyName="alternateCOCodesUIList" p:readOnly="@{!#dp.selectCrossListingAllowed}" p:render="@{!#dp.crossListedCourseCodes.isEmpty()}">
        <property name="label" value="Cross List As"/>
        <property name="control">
            <bean parent="Uif-HorizontalCheckboxesControl"/>
        </property>
        <property name="optionsFinder">
            <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.populateCrossCourseListKeyValues" p:addBlankOption="false" />
        </property>
    </bean>

    <bean id="KS-GradingAndRegOptions" parent="Uif-HorizontalBoxSubSection">
        <property name="items">
            <list>
                <ref bean="KS-CourseOfferingEdit-GradingOptions" />
                <ref bean="KS-CourseOfferingEdit-StudentRegOptions" />
                <ref bean="KS-CourseOfferingEdit-CreditType" />
            </list>
        </property>
    </bean>
    <bean id="KS-CourseOfferingEdit-GradingOptions" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <bean parent="Uif-InputField" p:propertyName="courseOfferingInfo.gradingOptionId" p:id="gradingOptionId" p:label="Grading Options" p:fieldLabel.renderColon="false" p:labelPlacement="TOP" p:componentSecurity.editAuthz="true">
                    <property name="control">
                        <bean parent="Uif-VerticalRadioControl"/>
                    </property>
                    <property name="optionsFinder">
                        <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.GradingOptionsKeyValues" p:addBlankOption="false" />
                    </property>
                </bean>
            </list>
        </property>
    </bean>
    <bean id="KS-CourseOfferingEdit-StudentRegOptions" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <!-- Commented out - List of checkboxes seem to break in KRAD 2.2M3 so had to replace with individual checkboxes -->
                <!--<bean parent="Uif-InputField-LabelTop" p:propertyName="courseOfferingInfo.studentRegistrationGradingOptions"-->
                <!--p:label="Student Registration Options" p:render="@{!#dp.studentRegOptions.empty}"-->
                <!-->-->
                <!--<property name="control">-->
                <!--<bean parent="Uif-VerticalCheckboxesControl"/>-->
                <!--</property>-->
                <!--<property name="optionsFinder">-->
                <!--<bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.StudentRegistrationOptionsKeyValues"-->
                <!--p:addBlankOption="false" />-->
                <!--</property>-->
                <!--</bean>-->
                <bean parent="Uif-MessageField" p:label="Student Registration Options"
                      p:labelPlacement="TOP" p:fieldLabel.renderColon="false"
                      p:render="true" />
                <!-- List of checkboxes seem to break in KRAD 2.2M3 so had to replace with individual checkboxes -->
                <bean parent="Uif-CheckboxInputField" p:label="Pass/Fail"
                      p:propertyName="passFailStudentRegOpts"
                      p:labelPlacement="RIGHT" p:fieldLabel.renderColon="false"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_ROUTE] and #dp.studentRegOptions.contains('kuali.resultComponent.grade.passFail')}"/>
                <bean parent="Uif-CheckboxInputField" p:label="Audit"
                      p:propertyName="auditStudentRegOpts"
                      p:labelPlacement="RIGHT" p:fieldLabel.renderColon="false"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_ROUTE] and #dp.studentRegOptions.contains('kuali.resultComponent.grade.audit')}"/>
                <!-- For read only view show as message fields -->
                <bean parent="Uif-MessageField"
                      p:messageText="Pass/Fail" p:render="@{!#actionFlags[#Constants.KUALI_ACTION_CAN_ROUTE] and #dp.passFailStudentRegOpts}"/>
                <bean parent="Uif-MessageField"
                      p:messageText="Audit" p:render="@{!#actionFlags[#Constants.KUALI_ACTION_CAN_ROUTE] and #dp.auditStudentRegOpts}"/>
                <!-- No reg options were available -->
                <bean parent="Uif-MessageField"
                      p:messageText="None available" p:render="@{#dp.studentRegOptions.empty}"/>
            </list>
        </property>
    </bean>
    <bean id="KS-CourseOfferingEdit-CreditType" parent="Uif-VerticalBoxSection"
          p:style="margin-left:15px;">
        <property name="items">
            <list>
                <!-- Credit Option Type Selector -->
                <bean parent="Uif-InputField-LabelTop" p:propertyName="creditOption.typeKey" p:label="Credit Type">
                    <property name="id" value="KS-CourseOfferingEdit-CreditType_OptionTypeSelector" />
                    <property name="control" >
                        <bean parent="Uif-HorizontalRadioControl" p:disabled="@{#dp.creditOptionFixed}" />
                    </property>
                    <property name="optionsFinder" >
                        <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.CreditOptionsKeyValues" p:addBlankOption="false" />
                    </property>
                </bean>
                <!-- Fixed Type for multiple -->
                <bean parent="Uif-InputField" p:label="Credits" p:propertyName="creditOption.fixedCredit" p:fieldLabel.renderColon="false"
                      p:style="margin-left: 1em;" p:progressiveRender="@{#dp.creditOption.typeKey eq 'kuali.result.values.group.type.fixed'}"
                      p:exclusiveMin="@{document.oldMaintainableObject.dataObject.creditOption.courseMinCredits}-0.0001"
                      p:inclusiveMax="@{document.oldMaintainableObject.dataObject.creditOption.courseMaxCredits}"  >
                    <property name="id" value="KS-CourseOfferingEdit-CreditType_OptionTypeFixed" />
                    <property name="control">
                        <bean parent="Uif-DropdownControl"/>
                    </property>
                    <property name="optionsFinder">
                        <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.MultipleCreditOptionsKeyValues" p:addBlankOption="false" />
                    </property>
                </bean>
                <!-- Fixed Type for variable -->
                <!-- comment the bean out because if fixed radio button is checked, you can only select credit from the dropdown instead of entering one using input field -->
                <!--<bean parent="Uif-InputField" p:label="Credits" p:propertyName="creditOption.fixedCredit" p:fieldLabel.renderColon="false"
                      p:style="margin-left: 1em;" p:progressiveRender="@{#dp.creditOption.typeKey eq 'kuali.result.values.group.type.fixed' and #dp.creditOption.minCredits ne null}"
                      p:exclusiveMin="@{document.oldMaintainableObject.dataObject.creditOption.courseMinCredits}-0.0001"
                      p:inclusiveMax="@{document.oldMaintainableObject.dataObject.creditOption.courseMaxCredits}"  >
                    <property name="id" value="KS-CourseOfferingEdit-CreditType_OptionTypeFixed" />
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="3" p:disabled="@{#dp.creditOptionFixed}"
                              p:onDocumentReadyScript="validateCredits(this, '@{#ConfigProperties['application.url']}', @{#dp.course.creditOptions[0].typeKey eq 'kuali.result.values.group.type.multiple'});"/>
                    </property>
                </bean>-->
                <!-- Range -->
                <bean parent="Uif-HorizontalBoxSection" p:style="margin-left: 1em;" p:progressiveRender="@{#dp.creditOption.typeKey eq 'kuali.result.values.group.type.range'}">
                    <property name="items">
                        <list>
                            <bean parent="Uif-InputField" p:label="Minimum Credits" p:exclusiveMin="@{document.oldMaintainableObject.dataObject.creditOption.courseMinCredits}-0.0001"
                                  p:propertyName="creditOption.minCredits" p:fieldLabel.renderColon="false" p:required="true">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="3" />
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:label="Maximum Credits" p:inclusiveMax="@{document.oldMaintainableObject.dataObject.creditOption.courseMaxCredits}"
                                  p:propertyName="creditOption.maxCredits" p:fieldLabel.renderColon="false" p:required="true">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="3" />
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <!-- Multiple -->
                <bean parent="Uif-InputField" p:readOnlyDisplayReplacement="@{#dp.creditOptionCreditsUI}" p:propertyName="creditOption.credits" p:label="Credits" p:fieldLabel.renderColon="false" p:style="margin-left: 1em;" p:progressiveRender="@{#dp.creditOption.typeKey eq 'kuali.result.values.group.type.multiple'}">
                    <property name="control">
                        <bean parent="Uif-HorizontalCheckboxesControl"/>
                    </property>
                    <property name="optionsFinder">
                        <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.MultipleCreditOptionsKeyValues" p:addBlankOption="false" />
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="KS-CourseOfferingEdit-DeliveryAndAssessment" parent="Uif-VerticalBoxSection" p:headerText="Delivery and Assessment">
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="&lt;hr&gt;"/>

                <bean parent="Uif-VerticalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="Uif-InputField" p:propertyName="courseOfferingInfo.finalExamType" p:id="finalExamType" p:label="Final Exams"
                                  p:fieldLabel.renderColon="false" p:labelPlacement="TOP" p:componentSecurity.editAuthz="true">
                                <property name="control">
                                    <bean parent="Uif-VerticalRadioControl"/>
                                </property>
                                <property name="optionsFinder">
                                    <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.FinalExamOptionsKeyValues" p:addBlankOption="false" />
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>

                <ref bean="KS-CourseOfferingEdit-DeliveryFormats"/>


            </list>
