		<property name="propertyName" value="targetTermId" />
		<property name="label" value="Term &amp; Year" />
		<property name="instructionalText" value="Note: This course MAY NOT be offered in your selected term. Check back after the schedule of classes has been released." />
		<property name="control">
			<bean parent="Uif-DropdownControl"/>
		</property>
		<property name="optionsFinder">
			<bean class="org.kuali.student.ap.planner.util.TermsListBuilder"/>
		</property>
	</bean>

    <bean id="planner_course_code_input" parent="planner_course_code_input-parent"/>
	<bean id="planner_course_code_input-parent" abstract="true" parent="planner_dialog_input">
		<property name="propertyName" value="courseCd" />
		<property name="label" value="Enter subject and catalog number" />
		<property name="control">
			<bean parent="Uif-TextControl" p:additionalCssClasses="ksap-planner-courseCode">
                <property name="onDocumentReadyScript" value="focusOnElement(jQuery('#courseCode_control'));" />
				<property name="watermarkText" value="e.g. BL MATH-M 119" />
				<property name="maxLength" value="50" />
				 <property name="size" value ="25"/>
			</bean>
		</property>
	</bean>

