    <bean id="planned_menu_items-POC-delay-load" parent="planned_menu_items-parent-POC-delay-load"/>
    <bean id="planned_menu_items-parent-POC-delay-load" abstract="true" parent="Uif-VerticalBoxSubSection">
        <property name="layoutManager.cssClasses">
            <list merge="true">
                <value>ks-plan-ContextMenu</value>
            </list>
        </property>
        <property name="hidden" value="true"/>
        <property name="items">
            <list>

                <ref bean="menuOptionCourseSummary"/>

                <ref bean="menuOptionEditPlanItem"/>


                <bean parent="menuOptionMarkCart">
                    <property name="onClickScript" value="
                        var additionalFormData = {viewId:'PlannedCourse-FormView', methodToCall:'plannedToCart', planItemId:jQuery(this).data('planitemid')};
                        submitHiddenForm('plan', additionalFormData, e);
                    "/>
                </bean>

                <bean parent="Uif-Message" p:messageText="Will be moved out of the Total Credit"
                      p:cssClasses="ks-plan-ContextMenu-note"/>


                <ref bean="menuOptionMarkBackup"/>

                <bean parent="Uif-Message" p:messageText="Will be moved below the Total Credit line"
                      p:cssClasses="ks-plan-ContextMenu-note"/>

                <ref bean="menuOptionMovePlanItem"/>

                <ref bean="menuOptionCopyPlanItem"/>

                <ref bean="menuOptionDeletePlanItem-POC-delay-load"/>

                <bean parent="Uif-Message" p:messageText="Cannot Undo" p:style="margin:0;"
                      p:cssClasses="ks-plan-ContextMenu-note"/>

