        <property name="cssClasses">
            <list merge="true">
                <value>ksap-noMargin</value>
            </list>
        </property>
        <property name="methodToCall" value="deleteDialogPOC"/>
        <!--<property name="actionScript" value="
            var retrieveData = {action:'plan', viewId:'PlannedCourse-FormView', methodToCall:'startDeleteDialog', planItemId:jQuery(this).data('planitemid'), atpId:jQuery(this).data('atpid'), pageId:'plan_item_delete_page'};
            var popupStyle = {width:'300px', height:'16px'};
            var popupOptions = {tail:{hidden:true}, align:'top', close:true};
            var pageId ='plan_item_delete_page.'+jQuery(this).data('planitemid').replace(/\./g, '-');
