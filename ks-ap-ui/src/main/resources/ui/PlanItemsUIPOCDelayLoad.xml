                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
        <property name="collectionLookup.render" value="false"/>
        <property name="collectionLookup.quickfinderAction.render" value="false"/>
        <property name="footer">
            <bean parent="Uif-VerticalBoxGroup">
                <property name="validationMessages.render" value="false"/>
                <property name="items">
                    <list>
                        <bean parent="term_total_credits"
                              p:render="@{#line.academicRecord.size() gt 0 and #line.openForPlanning}">
                            <property name="cssClasses">
                                <list merge="true">
                                    <value>ksap-carousel-term-total</value>
                                    <value>ks-plan-Bucket-credit</value>
                                </list>
                            </property>
                            <property name="items">
                                <list>
                                    <ref bean="term_total_credits_label"/>
                                    <ref bean="total_credits_help_icon"/>
                                    <ref bean="term_total_credits_text"/>
                                </list>
                            </property>
                        </bean>
                        <bean parent="term_total_credits"
                              p:render="@{#line.academicRecord.size() eq 0 and #line.openForPlanning}">
                            <property name="cssClasses">
                                <list merge="true">
                                    <value>ksap-carousel-term-total</value>
                                    <value>ks-plan-Bucket-credit</value>
                                </list>
                            </property>
                            <property name="items">
                                <list>
                                    <ref bean="term_total_credits_label"/>
                                    <ref bean="total_credits_help_icon"/>
                                    <ref bean="term_total_credits_text"/>
                                </list>
                            </property>
                        </bean>
                        <bean parent="term_no_courses"
                              p:render="@{!#line.openForPlanning and #line.plannedList.size() eq 0}"/>
                    </list>
                </property>
            </bean>
        </property>
    </bean>

    <bean id="menuOptionDeletePlanItem-POC-delay-load" parent="menuOptionDeletePlanItem-parent-POC-delay-load"/>
    <bean id="menuOptionDeletePlanItem-parent-POC-delay-load" abstract="true" parent="Uif-ActionLink" p:actionLabel="Delete from Plan"
          p:title="Delete from Plan">
