<?xml version="1.0" encoding="UTF-8" ?>
<!-- Use this one for deploying to JBoss -->
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
	http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">

	<!-- JPA test "unit" -->
    <persistence-unit name="persistentUnit"
		transaction-type="JTA">

        <class>org.kuali.student.r2.core.organization.model.OrgEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgAttributeEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgHierarchyEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgHierarchyAttributeEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgOrgRelationEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgOrgRelationAttributeEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgOrgRelationEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgOrgRelationAttributeEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgPersonRelationEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgPersonRelationAttributeEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgPositionRestrictionEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgPositionRestrictionAttributeEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgCodeEntity</class>
        <class>org.kuali.student.r2.core.organization.model.OrgCodeAttributeEntity</class>

        <exclude-unlisted-classes>true</exclude-unlisted-classes>

    </persistence-unit>

</persistence>
